local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({
    Name = "Lua X Scripthub",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "OrionTest",
    IntroEnabled = true,      -- Enable the intro animation
    IntroText = "Lua X Scripthub", -- Set the intro text
    IntroIcon = "",           -- Leave empty if no icon is needed
    Icon = "",                -- Leave empty if no window icon is needed
    CloseCallback = function() print("Window closed") end
})

-- Credits Tab
local CreditsTab = Window:MakeTab({
    Name = "Credits",
    Icon = "rbxassetid://4483345998",  -- Use valid icon or leave empty if not needed
    PremiumOnly = false
})
CreditsTab:AddParagraph("Developer", "DJFunk")

